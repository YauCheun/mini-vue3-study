{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es2021",
      "noImplicitAny": false,
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "downlevelIteration": true,
      "strict": false,
      "alwaysStrict": false,
      "outDir": "./dist"
  },
  "include": [
      "**/*"
  ],
  "exclude": []
}